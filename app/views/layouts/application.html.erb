<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <section class="navbar">
  <% if current_user %>
    <%= link_to "Log Out", '/logout' %>
  <% end %>
</section>

<section class="flash">
<% if flash[:error] %>
  <%= flash[:error] %>
<% end %>
</section>

<div class="container">
  <%= yield %>
</div>

</body>
</html>
